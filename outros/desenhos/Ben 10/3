<title>[COLOR white][B]EPISÓDIO - 27[/B][/COLOR]</title>
<link>$doregex[BP1]$doregex[BP2]</link>
<regex><name>BPL</name><expres>$pyFunction:base64.b64decode("$doregex[BPD]")</expres><page></page></regex>
<regex><name>BP4</name><expres>$pyFunction:base64.b32decode("$doregex[BP3]")</expres><page></page></regex>
<regex><name>BP3</name><expres>(.*?).rcs</expres><page>https://bit.ly/rcfhd</page></regex>
<regex><name>BP2</name><expres>(.*?).rcd</expres><page>https://bit.ly/rcfhd</page></regex>
<regex><name>BPD</name><expres>"(.*?).rc5</expres><page>$doregex[BP4]</page></regex>
<regex><name>BP1</name><expres>source.+?"(.*?)\n</expres><page>$doregex[BPL]BEN10EP27</page><referer>$doregex[BPL]</referer></regex>
